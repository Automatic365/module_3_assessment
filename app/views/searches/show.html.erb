<h1> Search Results</h1>

<h3><%= @all %> Total Stores</h3>


  <table class="table">
    <tr>
      <th>Name</th>
      <th>City</th>
      <th>Distance</th>
      <th>Phone Number</th>
      <th>Store Type</th>
    </tr>

    <% @stores.each do |store| %>
      <tr>
        <td><%= store.longName %></td>
        <td><%= store.city %></td>
        <td><%= store.distance %></td>
        <td><%= store.phone %></td>
        <td><%= store.storeType %></td>
      </tr>
    <% end %>
  </table>
